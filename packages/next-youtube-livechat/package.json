{
  "name": "next-youtube-livechat",
  "version": "0.1.29",
  "private": false,
  "description": "Fetch YouTube live chat without API using NextJS + React Hook which bypass CORS.",
  "author": "Sawa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sawaYch/next-youtube-livechat"
  },
  "homepage": "https://next-youtube-livechat.vercel.app",
  "keywords": [
    "NextJS",
    "React",
    "YouTube",
    "CORS",
    "Youtube API",
    "livechat",
    "hook"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "source": "./src/index.ts",
  "scripts": {
    "dev": "tsup src/index.ts --format esm,cjs --sourcemap --dts --minify --watch",
    "build": "tsup src/index.ts --format esm,cjs --sourcemap --dts --minify",
    "lint": "eslint --ext .ts,.tsx",
    "format": "npx prettier -c \"**/*.{ts,tsx,css}\"",
    "format:fix": "npx prettier -w \"**/*.{ts,tsx,css}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "next": "^14.1.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.1",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/eslint": "^8.56.5",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.3.0",
    "tsup": "^8.0.2",
    "typescript": "^5"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
